<!DOCTYPE html>
<html>
<head>
    <title>DepoiT: @ViewBag.Title</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
    <div class="row">
        <header class="container-fluid">
            <nav class="navbar navbar-dark bg-dark navbar-expand-md">
                <h1 class="navbar-brand"><i class="fas fa-paint-brush"></i>DepoiT</h1>
                <button class="navbar-toggler" data-toggle="collapse" data-target="#theMenu">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div id="theMenu" class="navbar-collapse collapse">
                    <ul class="navbar-nav">
                        <li class="nav-item"><a class="nav-link" asp-controller="Dashboard" asp-action="Index">Dashboard</a></li>
                        <li class="nav-item"><a class="nav-link" asp-controller="Wizard" asp-action="Index">Wizard</a></li>
                        <li class="nav-item"><a class="nav-link" href="swagger/index.html">Swagger</a></li>
                    </ul>
                </div>
            </nav>
        </header>
        <section class="container">
            <h2 class="text-center">@ViewBag.Title</h2>
            @RenderBody()
        </section>
        <footer class="container">
            <div class="text-center">Copyright 2021 DepoiT</div>
        </footer>
    </div>

    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/fortawesome/fontawesome-free/js/fontawesome.js"></script>
    <script src="~/lib/fortawesome/fontawesome-free/js/solid.js"></script>
    @RenderSection("scripts", false)
</body>
</html> 